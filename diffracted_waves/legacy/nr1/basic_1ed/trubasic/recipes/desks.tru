    DATA 2, 8, 24, 14, 32, 27, 3, 9, 19, 13, 30, 6, 22, 11, 4, 25

    ! This array is read in Fortran order; can't use MAT READ

    FOR kk = 1 to 8
        FOR j = 1 to 4
            FOR i = 1 to 16
                READ iq(i, j, kk)
            NEXT i
        NEXT j
    NEXT kk

    DATA 14, 4, 13, 1, 2, 15, 11, 8, 3, 10, 6, 12, 5, 9, 0, 7
